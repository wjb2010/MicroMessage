<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/rel0"

        >
        <EditText
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:hint="请输入电话号"
            android:gravity="center_horizontal|center_vertical"
            android:id="@+id/et1"

            />



    </RelativeLayout>


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/rel1"
android:layout_below="@+id/rel0"
        >

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jg"
            android:id="@+id/btn1"

            android:background="@android:color/transparent"

            />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jh"

            android:id="@+id/btn2"
            android:layout_toRightOf="@+id/btn1"
            android:background="@android:color/transparent"
            />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/ji"
            android:id="@+id/btn3"
            android:layout_toRightOf="@+id/btn2"
            android:background="@android:color/transparent"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/rel2"
        android:layout_below="@+id/rel1"
        >

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jk"
            android:id="@+id/btn5"
            android:layout_toRightOf="@+id/btn4"
            android:background="@android:color/transparent"
            />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jl"
            android:id="@+id/btn6"
            android:layout_toRightOf="@+id/btn5"
            android:background="@android:color/transparent"
            />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jj"
            android:id="@+id/btn4"
            android:background="@android:color/transparent"

            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/rel3"
        android:layout_below="@+id/rel2"
        >
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jm"
            android:id="@+id/btn7"
            android:background="@android:color/transparent"

            />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jn"
            android:id="@+id/btn8"
            android:layout_toRightOf="@+id/btn7"
            android:background="@android:color/transparent"
            />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jo"
            android:id="@+id/btn9"
            android:layout_toRightOf="@+id/btn8"
            android:background="@android:color/transparent"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/rel4"
        android:layout_below="@+id/rel3"
        >
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
           android:src="@mipmap/jp"
            android:id="@+id/btn10"
            android:background="@android:color/transparent"

            />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/jf"
            android:id="@+id/btn11"
            android:layout_toRightOf="@+id/btn10"
            android:background="@android:color/transparent"
            />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:src="@mipmap/kd"
            android:id="@+id/btn12"
            android:layout_toRightOf="@+id/btn11"
            android:background="@android:color/transparent"
            />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/rel5"
        android:layout_below="@+id/rel4"
        >
        <Button
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:text="打电话"
            android:id="@+id/call"
android:layout_centerInParent="true"

            />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="70dp"
           android:src="@mipmap/a9h"
            android:id="@+id/back"
            android:layout_marginLeft="50dp"
android:layout_toRightOf="@+id/call"
            android:background="@android:color/transparent"

            />

    </RelativeLayout>

</RelativeLayout>
